<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">
        <record id="repair_product_category" model="product.category">
            <field name="parent_id" ref="product.cat0"/>
            <field name="name">Repairable</field>
        </record>
        <record id="product_product_repair_generic" model="product.product">
            <field name="list_price">10.00</field>
            <field name="standard_price">10.0</field>
            <field name="uom_id" ref="product.product_uom_unit"/>
            <field name="uom_po_id" ref="product.product_uom_unit"/>
            <field name="type">product</field>
            <field name="name">Generic Repair Product</field>
            <field name="categ_id" ref="repair_product_category"/>
        </record>
        <record forcecreate="True" id="property_repair_product" model="ir.property">
            <field name="name">property_repair_product</field>
            <field name="fields_id" search="[('model','=','res.company'),('name','=','property_repair_product')]"/>
            <field eval="'product.product,'+str(product_product_repair_generic)" name="value"/>
            <field name="company_id" ref="base.main_company"/>
        </record>
        
    </data>
</openerp>